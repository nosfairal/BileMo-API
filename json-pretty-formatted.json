{
    "openapi": "3.0.0",
    "info": {
        "title": "BileMo App",
        "description": "Access BileMo products and manage your API users",
        "version": "1.0.0"
    },
    "paths": {
        "\/api\/login_check": {
            "post": {
                "tags": [
                    "Login"
                ],
                "summary": "Login into the api",
                "requestBody": {
                    "content": {
                        "application\/json": {
                            "schema": {
                                "properties": {
                                    "userName": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Get the Bearer token to log in the API",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid credentials"
                    },
                    "400": {
                        "description": "Bad Json syntax or incorrect data"
                    }
                },
                "security": []
            }
        },
        "\/api\/products\/{id}": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get details of a product",
                "operationId": "get_product_details",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns a product",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/Product"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Product not found"
                    }
                }
            }
        },
        "\/api\/api\/customers\/{customerId}\/user\/{userId}": {
            "delete": {
                "tags": [
                    "Users"
                ],
                "operationId": "delete_delete_user",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Delete a user"
                    },
                    "401": {
                        "description": "Unauthorized request"
                    },
                    "404": {
                        "description": "User not found"
                    }
                }
            }
        },
        "\/api\/products": {
            "get": {
                "summary": "Get list of BileMo products",
                "operationId": "get_products_list",
                "responses": {
                    "200": {
                        "description": "Returns the list of products",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/Product2"
                                }
                            }
                        }
                    }
                }
            }
        },
        "\/api\/customers\/{customerId}\/users": {
            "get": {
                "summary": "Get list of your organization's users",
                "operationId": "get_users_list",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns the list of your users"
                    }
                }
            }
        },
        "\/api\/customers\/{customerId}\/users\/{userId}\/details": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get details of a user",
                "operationId": "get_users_details",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns a user"
                    },
                    "401": {
                        "description": "Unauthorized request"
                    },
                    "404": {
                        "description": "User not found"
                    }
                }
            }
        },
        "\/api\/customers\/{customerId}\/user\/create": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Add a new user for your organization",
                "operationId": "post_user_create",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "The new user to create",
                    "required": true,
                    "content": {
                        "application\/Json": {
                            "schema": {
                                "properties": {
                                    "userName": {
                                        "description": "UserName for user identification",
                                        "type": "string"
                                    },
                                    "password": {
                                        "description": "User's choosen password",
                                        "type": "string"
                                    },
                                    "first_name": {
                                        "description": "User's first name",
                                        "type": "string"
                                    },
                                    "last_name": {
                                        "description": "User's last name",
                                        "type": "string"
                                    },
                                    "email": {
                                        "description": "User's email address",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Create a user and returns it"
                    },
                    "400": {
                        "description": "Bad Json syntax or incorrect data"
                    },
                    "401": {
                        "description": "Unauthorized request"
                    }
                }
            }
        },
        "\/api\/customers\/{customerId}\/user\/{userId}": {
            "delete": {
                "tags": [
                    "Users"
                ],
                "operationId": "delete_users_delete",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Delete a user"
                    },
                    "401": {
                        "description": "Unauthorized request"
                    },
                    "404": {
                        "description": "User not found"
                    }
                }
            },
            "patch": {
                "tags": [
                    "Users"
                ],
                "summary": "Update a user",
                "operationId": "patch_user_update",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "\\d+"
                        }
                    }
                ],
                "requestBody": {
                    "description": "The user data you want to update.If you don't want to change a field don't mention it",
                    "content": {
                        "application\/Json": {
                            "schema": {
                                "properties": {
                                    "userName": {
                                        "description": "User's userName",
                                        "type": "string"
                                    },
                                    "password": {
                                        "description": "User's choosen password",
                                        "type": "string"
                                    },
                                    "first_name": {
                                        "description": "User's first name",
                                        "type": "string"
                                    },
                                    "last_name": {
                                        "description": "User's last name",
                                        "type": "string"
                                    },
                                    "email": {
                                        "description": "User's email address",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Update a user and returns it"
                    },
                    "400": {
                        "description": "Bad Json syntax or incorrect data"
                    },
                    "401": {
                        "description": "Unauthorized request"
                    },
                    "404": {
                        "description": "User not found"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Product": {
                "properties": {
                    "_links": {
                        "properties": {
                            "products:list": {
                                "properties": {
                                    "href": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "readOnly": true
                    }
                },
                "type": "object"
            },
            "Product2": {
                "properties": {
                    "_links": {
                        "properties": {
                            "self": {
                                "properties": {
                                    "href": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "readOnly": true
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "Bearer": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "security": [
        {
            "Bearer": []
        }
    ]
}
